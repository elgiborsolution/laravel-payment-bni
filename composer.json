{
  "name": "eligibor-solution/laravel-payment-bni",
  "description": "Laravel package for BNI eCollection (VA) & QRIS with auditing, granular errors, tests & Postman.",
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Elgibor Solution",
      "email": "dev@elgibor-solution.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": "^11.0 || ^12.0",
    "illuminate/http": "^11.0 || ^12.0",
    "illuminate/contracts": "^11.0 || ^12.0",
    "guzzlehttp/guzzle": "^7.9"
  },
  "require-dev": {
    "orchestra/testbench": "^9.0 || ^10.0 || ^11.0",
    "phpunit/phpunit": "^11.0 || ^12.0"
  },
  "autoload": {
    "psr-4": {
      "ESolution\\BNIPayment\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ESolution\\BNIPayment\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ESolution\\BNIPayment\\BNIPaymentServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}